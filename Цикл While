numbers = [42, 69, 322, 13, 0, 99, -5, 9, 8, 7, -6, 5]
index = 0

while index < len(numbers): #сравниваем индекс числа и длину списка
    if numbers[index] > 0: #Если индекс числа больше 0
        print(numbers[index]) #Выводим число индекса
        index = index + 1 #увеличиваем индекс + 1 - повторяем цикл
    elif numbers[index] <= 0: #если на пути встретилось число 0 или отрицательное число, сравниваем
        index = index + 1 #увеличиваем следующий индекс так как число не подошло под условия
        print(numbers[index]) # выводим число. 
        break #Отрицательное число прекращает работу цикла
